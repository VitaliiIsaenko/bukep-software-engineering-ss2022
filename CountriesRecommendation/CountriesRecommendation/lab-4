public bool Satisfied(Country country)

{

   return LiveBySeaSatisfied(country);

}



private bool LiveBySeaSatisfied(Country country) 

{
    if (liveBySea)

        {

            if (country.HasSea)

            {

                return true;

            }

            else

            {

                return false;

            }

        }

        else

        {

            return true;

        }
}